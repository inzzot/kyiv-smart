        <activiti:formProperty id="cc_1_3" name="Галка на прінтформі" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="markersUser1" name="Маркер адреса стартТаски" type="markers" default="${markerService.loadFromFile('soczah/dnepr_soczah_163_user.json')}"></activiti:formProperty>

        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
