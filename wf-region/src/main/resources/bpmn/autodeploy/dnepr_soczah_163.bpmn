        <activiti:formProperty id="post_office" name="№ відділення поштового зв'язку (індекс); ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="bank_name" name="Назва банку; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="mfo" name="МФО банку; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="edrpou" name="ЄДРПОУ банку; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="bank_account" name="Номер рахунку (р/р); ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="decision_1" name="Перевірка данних" type="enum" required="true">
          <activiti:value id="accept" name="Заповнені дані вірні, інформація направлена на розгляд, очікується рішення"></activiti:value>
          <activiti:value id="accept_but" name="Дані надані не в повному обсязі - направити громаднину повідомлення (обов'язково впишіть коментар нижче)"></activiti:value>
          <activiti:value id="reject" name="Дані заповнені невірно, відхилити звернення (обов'язково впишіть коментар нижче)"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="comment_1" name="У разі відхилення запиту внесіть, будь ласка, коментар" type="textArea"></activiti:formProperty>

