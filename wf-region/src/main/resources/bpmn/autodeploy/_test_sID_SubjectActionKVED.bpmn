<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="_test_sID_SubjectActionKVED" name="Тестування поля для роботи зі справочником КВЕДів" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="email" name="Вкажіть E-Mail адресу для зворотнього зв'язку" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sCurrency_1" name="sCurrency_1" type="string"></activiti:formProperty>
        <activiti:formProperty id="sObjectCustoms_1" name="sObjectCustoms_1" type="string"></activiti:formProperty>
        <activiti:formProperty id="sSubjectOrganJoinTax_1" name="sSubjectOrganJoinTax_1" type="string"></activiti:formProperty>
        <activiti:formProperty id="sObjectEarthTarget_1" name="sObjectEarthTarget_1" type="string"></activiti:formProperty>
        <activiti:formProperty id="sCountry_1" name="sCountry_1" type="string"></activiti:formProperty>
        <activiti:formProperty id="sID_SubjectActionKVED_1" name="sID_SubjectActionKVED_1" type="string"></activiti:formProperty>
<!--         <activiti:formProperty id="sID_ObjectPlace_UA_1" name="sID_ObjectPlace_UA_1" type="string"></activiti:formProperty>

        <activiti:formProperty id="Info_st_0" name="= Тест маркера ValuesFieldsOnCondition начало ==============================" type="label" default="======"></activiti:formProperty>
          -->
          <activiti:formProperty id="sVarTest_1" name="sVarTest_1" type="string" default="TestString"></activiti:formProperty>
        <activiti:formProperty id="sVarTest_2" name="sVarTest_2" type="string"></activiti:formProperty>
        <activiti:formProperty id="sVarTest_3" name="sVarTest_3" type="string"></activiti:formProperty>
        <activiti:formProperty id="sVarTest_4" name="sVarTest_4" type="string" default="55"></activiti:formProperty>
        <activiti:formProperty id="sVarTest_5" name="sVarTest_5" type="string"></activiti:formProperty>
        <activiti:formProperty id="sVarTest_6" name="sVarTest_6" type="string" default="sVarTest_6"></activiti:formProperty>
        <activiti:formProperty id="sVarTest_7" name="sVarTest_7" type="string"></activiti:formProperty>
        
        <activiti:formProperty id="asTestEnum" name="asTestEnum-Присвоить значение" type="enum" default="enVarTest_1" >
          <activiti:value id="enVarTest_1" name="sVarTest_1 + sVarTest_4 + asTestEnum + _ + email + sID_SubjectActionKVED_1"></activiti:value>
          <activiti:value id="enTestNumber" name="sVarTest_4 + asTestEnum + email + sVarTest_1 + sID_SubjectActionKVED_1 + _"></activiti:value>
          <activiti:value id="enTestNumber2" name="email + _ + sID_SubjectActionKVED_1 "></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="markers_st_1" name="Присвоить значение sVarTest_1 + sVarTest_4 + asTestEnum + _ + email + sID_SubjectActionKVED_1" type="markers" default="{&quot;motion&quot;:{&quot;ValuesFieldsOnCondition_st_1&quot;:{&quot;aField_ID&quot;:[&quot;sVarTest_2&quot;,&quot;sVarTest_3&quot;,&quot;sVarTest_4&quot;,&quot;sVarTest_5&quot;,&quot;sVarTest_6&quot;,&quot;sVarTest_7&quot;],&quot;asID_Field_sValue&quot;:[&quot;[sVarTest_1]&quot;,&quot;[sVarTest_4]&quot;,&quot;[asTestEnum]&quot;,&quot;_&quot;,&quot;[email]&quot;,&quot;[sID_SubjectActionKVED_1]&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;asTestEnum&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'enVarTest_1'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="markers_st_2" name="Присвоить значение sVarTest_4 + asTestEnum + email + sVarTest_1 + sID_SubjectActionKVED_1 + _" type="markers" default="{&quot;motion&quot;:{&quot;ValuesFieldsOnCondition_st_2&quot;:{&quot;aField_ID&quot;:[&quot;sVarTest_2&quot;,&quot;sVarTest_3&quot;,&quot;sVarTest_4&quot;,&quot;sVarTest_5&quot;,&quot;sVarTest_6&quot;,&quot;sVarTest_7&quot;],&quot;asID_Field_sValue&quot;:[&quot;sVarTest_4&quot;,&quot;[asTestEnum]&quot;,&quot;[email]&quot;,&quot;[sVarTest_1]&quot;,&quot;[sID_SubjectActionKVED_1]&quot;,&quot;_&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;asTestEnum&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'enTestNumber'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="markers_st_3" name="Присвоить значение нескольких полей в sVarTest_2" type="markers" default="{&quot;motion&quot;:{&quot;ValuesFieldsOnCondition_st_3&quot;:{&quot;aField_ID&quot;:[&quot;sVarTest_2&quot;],&quot;asID_Field_sValue&quot;:[&quot;[email]+_+[sVarTest_1]&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;asTestEnum&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'enTestNumber2'&quot;}}}"></activiti:formProperty>
          <activiti:formProperty id="markers_st_4" name="Маркер-разделитель" type="markers" default="{&quot;attributes&quot;:{&quot;Line_1&quot;:{&quot;aElement_ID&quot;:[&quot;sCurrency_1&quot;,&quot;sVarTest_1&quot;],&quot;sValue&quot;:&quot;Общая группа&quot;}}}"></activiti:formProperty>
          <activiti:formProperty id="Info_st_1" name="= Тест маркера ValuesFieldsOnCondition конец =v2============================" type="label" default="======"></activiti:formProperty>
        
      </extensionElements>
    </startEvent>
    <userTask id="usertask1" name="User Task" activiti:candidateGroups="sales">
     <extensionElements>
        <activiti:formProperty id="sCurrency_1" name="sCurrency_1" type="string"></activiti:formProperty>
        <activiti:formProperty id="sObjectCustoms_1" name="sObjectCustoms_1" type="string"></activiti:formProperty>
        <activiti:formProperty id="sSubjectOrganJoinTax_1" name="sSubjectOrganJoinTax_1" type="string"></activiti:formProperty>
        <activiti:formProperty id="sObjectEarthTarget_1" name="sObjectEarthTarget_1" type="string"></activiti:formProperty>
        <activiti:formProperty id="sCountry_1" name="sCountry_1" type="string"></activiti:formProperty>
        <activiti:formProperty id="sID_SubjectActionKVED_1" name="sID_SubjectActionKVED_1" type="string"></activiti:formProperty>
        <activiti:formProperty id="sID_ObjectPlace_UA_1" name="sID_ObjectPlace_UA_1" type="string"></activiti:formProperty>

        <activiti:formProperty id="Info_ut1_0" name="Тест маркера RequiredFieldsOnCondition начало" type="label" default=" "></activiti:formProperty>
        <activiti:formProperty id="asTestEnum" name="asTestEnum-Установить свойство Required у поля sTestRequired в состояние - true" type="enum" default="NotReq" >
          <activiti:value id="Req" name="Req-Обязательное поле"></activiti:value>
          <activiti:value id="NotReq" name="NotReq-Необязательное поле"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="markers_Req_01" name="Сделать поле sTestRequired обязательным если asTestEnum=Req" type="markers" default="{&quot;motion&quot;:{&quot;RequiredFieldsOnCondition_01&quot;:{&quot;aField_ID&quot;:[&quot;sTestRequired&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;asTestEnum&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'Req'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="sTestRequired" name="sTestRequired" type="string" ></activiti:formProperty>
        <activiti:formProperty id="sTestRequired2" name="sTestRequired2" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="Info_ut1_1" name="Тест маркера RequiredFieldsOnCondition конец" type="label" default=" "></activiti:formProperty>
        
      </extensionElements>
    </userTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="usertask1" targetRef="endevent1"></sequenceFlow>
  </process>
  <bpmndi:BPMNDiagram id="BPMNDiagram__test_sID_SubjectActionKVED">
    <bpmndi:BPMNPlane bpmnElement="_test_sID_SubjectActionKVED" id="BPMNPlane__test_sID_SubjectActionKVED">
      <bpmndi:BPMNShape bpmnElement="startevent1" id="BPMNShape_startevent1">
        <omgdc:Bounds height="35.0" width="35.0" x="70.0" y="90.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="usertask1" id="BPMNShape_usertask1">
        <omgdc:Bounds height="55.0" width="105.0" x="160.0" y="80.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="endevent1" id="BPMNShape_endevent1">
        <omgdc:Bounds height="35.0" width="35.0" x="320.0" y="90.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="flow1" id="BPMNEdge_flow1">
        <omgdi:waypoint x="105.0" y="107.0"></omgdi:waypoint>
        <omgdi:waypoint x="160.0" y="107.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow2" id="BPMNEdge_flow2">
        <omgdi:waypoint x="265.0" y="107.0"></omgdi:waypoint>
        <omgdi:waypoint x="320.0" y="107.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</definitions>
