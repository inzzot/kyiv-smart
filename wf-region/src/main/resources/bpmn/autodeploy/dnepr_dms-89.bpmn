      <script>
      var src=execution.getVariable('date_of_visit')
var year=src.substr(0,4)
var month=src.substr(5,2)
var day=src.substr(8,2)
var minpay='12,33'
var maxpay='36,98'
if (month=='12')
    {
        minpay='13,18'
        maxpay='39,53'
    }
execution.setVariable('sMinPay', minpay)
execution.setVariable('sMaxPay', maxpay)
	  </script>
