    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var per4 = 'substance'
var per5 = 'substance_2'
var per6 = 'substance_3'
var per7 ='term'
if (execution.getVariable('nFlag') != '1'){
per7 = 'тридцяти календарних днів з дня реєстрації заяви'
}
if (execution.getVariable('sID_Public_SubjectOrganJoin') == 'don_mariupolT'){
per7 = '10 робочих днів з дня одержання дозвільним органом від  адміністратора заяви та документів, що додаються до неї'
}
if (execution.getVariable('sID_Public_SubjectOrganJoin') == 'hmel_hmelT'){
per7 = '10 робочих днів з дня реєстрації заяви'
}
if (execution.getVariable('vibor_service') == 'issue'){
per4 = 'видати'
per5 = 'Ваш дозвіл (санітарний паспорт) на право експлуатації рентгенівського кабінету (зберігання та експлуатації пересувних і переносних (палатних) рентгенівських апаратів) лікувально-профілактичних закладів - готовий'
per6 = 'видачі'
}
if (execution.getVariable('vibor_service') == 'restructuring'){
per4 = 'переоформити'
per5 = 'Ваш переоформлений дозвіл (санітарний паспорт) на право експлуатації рентгенівського кабінету (зберігання та експлуатації пересувних і переносних (палатних) рентгенівських апаратів) лікувально-профілактичних закладів - готовий'
per6 = 'переоформлення'
per7 = 'двох робочих днів з дня реєстрації заяви'
}
if (execution.getVariable('vibor_service') == 'duplicate'){
per4 = 'видати дублікат'
per5 = 'Ваш дублікат дозволу (санітарний паспорт) на право експлуатації рентгенівського кабінету (зберігання та експлуатації пересувних і переносних (палатних) рентгенівських апаратів) лікувально-профілактичних закладів - готовий'
per6 = 'видачі дублікату'
per7 = 'двох робочих днів з дня реєстрації заяви'
}
if (execution.getVariable('vibor_service') == 'cansel'){
per4 = 'анулювати'
per5 = 'Рішення про анулювання документа дозвільного характеру - готове'
per6 = 'анулювання'
per7 = 'п`яти робочих днів з дня реєстрації заяви'
}
execution.setVariable('substance', per4)
execution.setVariable('substance_2', per5)
execution.setVariable('substance_3', per6)
execution.setVariable('term', per7)
</script>
    </scriptTask>
