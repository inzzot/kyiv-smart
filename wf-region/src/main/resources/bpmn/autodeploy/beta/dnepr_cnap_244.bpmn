        <activiti:formProperty id="phone" name="Контактний телефон; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="email" name="Електронна пошта; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="vibor_service" name="Суть звернення" type="enum" writable="false">
