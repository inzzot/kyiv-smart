        <activiti:formProperty id="markers1" name="Видача дозволу Дніпро" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_1&quot;:{&quot;aField_ID&quot;:[&quot;info&quot;,&quot;nFile_1&quot;,&quot;nFile_3&quot;,&quot;nFile_4&quot;,&quot;nFile_5&quot;,&quot;nFile_6&quot;,&quot;nFile_11&quot;,&quot;nFile_12&quot;,&quot;nFile_13&quot;,&quot;nFile_14&quot;,&quot;nFile_15&quot;,&quot;nFile_16&quot;,&quot;nFile_17&quot;,&quot;nFile_18&quot;,&quot;nFile_19&quot;,&quot;nFile_20&quot;,&quot;nFile_21&quot;],&quot;asID_Field&quot;:{&quot;sClient1&quot;:&quot;vibor_service&quot;,&quot;sClient2&quot;:&quot;nFlag&quot;},&quot;sCondition&quot;:&quot;[sClient1] == 'issue' &amp;&amp;[sClient2] != '1'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="markers6" name="Видача дозволу Маріуполь" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_6&quot;:{&quot;aField_ID&quot;:[&quot;info&quot;,&quot;nFile_1&quot;,&quot;nFile_22&quot;],&quot;asID_Field&quot;:{&quot;sClient1&quot;:&quot;vibor_service&quot;,&quot;sClient2&quot;:&quot;sID_Public_SubjectOrganJoin&quot;},&quot;sCondition&quot;:&quot;[sClient1] == 'issue'&amp;&amp; [sClient2]== 'don_mariupolT'&quot;}}}"></activiti:formProperty>
