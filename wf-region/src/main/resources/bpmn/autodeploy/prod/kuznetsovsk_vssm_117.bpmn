<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="kusnetsovsk_vssm_117" name="Надання довідки про те, що посвідчення батьків та дитини з багатодітної сім'ї не видавалося" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="sID_Public_SubjectOrganJoin" name="Оберіть свій район або місто" type="select" default="0" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По батькові" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdAddressFactual" name="Адреса реєстрації громадянина" type="string"></activiti:formProperty>
        
        
        <activiti:formProperty id="phone" name="Укажіть контактний номер телефону" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-Mail адреса для зворотнього зв'язку" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="dovidka" name="Оберіть спосіб отримання довідки" type="enum" required="true">
          <activiti:value id="programa" name="Надіслати в електронному вигляді (скан-копія)"></activiti:value>
          <activiti:value id="other" name="Особисто"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="passport_father" name="Копія паспорту батька (1, 2, 10, 11-13 стор.)" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="passport_mother" name="Копія паспорту матері (1, 2, 10, 11-13 стор.)" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="passport_children" name="Копії свідоцтв про народження усіх дітей; завантажте єдиним архівом усі свідоцтва" type="file" required="true"></activiti:formProperty>


        <activiti:formProperty id="sNameArea" name="назва населеного пункту" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="sNameOrgan" name="назва органу" type="invisible" default=" "></activiti:formProperty>
        <activiti:formProperty id="sPhoneOrgan" name="телефон" type="invisible" default=" "></activiti:formProperty>
        <activiti:formProperty id="sWorkTime" name="графік роботи" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="sAddress" name="адреса органу" type="invisible" default=" "></activiti:formProperty>
        <activiti:formProperty id="sMailClerk" name="Пошта" type="invisible" default="valery.stavitsky@gmail.com"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <userTask id="usertask1" name="надання адміністративної послуги" activiti:candidateGroups="${sID_Public_SubjectOrganJoin}">
      <extensionElements>
        <activiti:formProperty id="sNameOrgan" name="назва органу" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="clFio" name="Автор звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний номер телефону" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-Mail адреса для зворотнього зв'язку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdAddressFactual" name="Адреса реєстрації громадянина; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="dovidka" name="Оберіть спосіб отримання довідки" type="enum" writable="false">
          <activiti:value id="programa" name="Надіслати в електронному вигляді (скан-копія)"></activiti:value>
          <activiti:value id="other" name="Особисто"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="decision_1" name="Результат послуги" type="enum" required="true">
          <activiti:value id="accept" name="довідка готова"></activiti:value>
          <activiti:value id="reject" name="відмова у наданні довідки"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="comment_1" name="У разі відмови у наданні довідки внесіть, будь ласка, причину" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="rishen" name="Завантажте копію документа" type="file" required="true"></activiti:formProperty>
        <!--  
        <activiti:formProperty id="sBody_1" name="[pattern/print/minsoc/kusnetsovsk_vssm_117_print.html]" type="invisible" default="Заява громадянина" writable="false"></activiti:formProperty>
         -->
        <activiti:formProperty id="sNameArea" name="назва населеного пункту" type="invisible"></activiti:formProperty>
        
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
        <activiti:taskListener event="create" delegateExpression="${fileTaskInheritance}">
          <activiti:field name="aFieldInheritedAttachmentID">
            <activiti:expression>${passport_father}, ${passport_mother}, ${passport_children}</activiti:expression>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <serviceTask id="servicetask4" name="Повідомлення співробітника держоргану" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${sMailClerk}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Нове  звернення. Надання довідки про те, що посвідчення батьків та дитини з багатодітної сім'ї не видавалося]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[
          	Звернення № [sID_Order]<br>
          	[pattern/mail/_common_employee_notify.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask3" name="Повідомлення громадянина про успішне подання звернення" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
		<activiti:field name="sPhone_SMS">
			<activiti:expression>${phone}</activiti:expression>
		</activiti:field>
		<activiti:field name="sText_SMS">
			<activiti:expression>Vashe zvernennya [sID_Order] zareestrovano</activiti:expression>
		</activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression>" "</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення на тему: надання довідки про те, що посвідчення батьків та дитини з багатодітної сім'ї не видавалося]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_header.html]
		    <h3>Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.</h3>
			<p>
			Ваше звернення № <a href="https://igov.org.ua/search?sID_Order=[sID_Order]">[sID_Order]</a>  щодо видачі надання довідки про те, що посвідчення батьків та дитини з багатодітної 
			сім'ї не видавалося отримано та буде опрацьовано протягом 10 робочих днів.<br />
			Ви отримаєте додаткове повідомлення за результатами розгляду запиту.
			</p>
			<br>
			[pattern/mail/_common_signature_start.html]
			${sNameOrgan}.<br>
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_footer.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask2" name="повідомлення громадянина про наданні довідки" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[test_noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA["${email}"]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення на тему: надання довідки про те, що посвідчення батьків та дитини з багатодітної сім'ї не видавалося]]></activiti:string>
        </activiti:field>
        <activiti:field name="sPhone_SMS">
          <activiti:expression><![CDATA[${phone}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sText_SMS">
          <activiti:expression><![CDATA[Status Vashoho zvernennya [sID_Order] zmineno na "Vzyato v robotu". Detali: igov.org.ua/journal abo u Vashomu email]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA["${rishen}"]]></activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_header.html]
		    <h3>Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.</h3>
			<p>
				Ваше звернення № <a href="https://igov.org.ua/search?sID_Order=[sID_Order]">[sID_Order]</a> щодо надання довідки про те, 
				що посвідчення батьків та дитини з багатодітної сім'ї не видавалося - опрацьовано.<br />
				Копія документу знаходиться у додатку до листа.
			</p>
			<br>
			Ви або уповноважена Вами особа (при наявності довіреності) можете отримати довідку собисто під підпис за адресою:
			${sAddress}<br>
			${sWorkTime}<br>
			${sPhoneOrgan}<br><br><br>			
			[pattern/mail/_common_feedback.html]
			[pattern/mail/_common_signature_start.html]
			${sNameOrgan}.<br>
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_footer.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask5" name="повідомлення громадянина про відмову у видачі довідки" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sPhone_SMS">
          <activiti:expression><![CDATA[${phone}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sText_SMS">
          <activiti:expression><![CDATA[Za Vashym zvernennyam [sID_Order] vyneseno vidmovu v nadanni poslugy. Detali: igov.org.ua/journal abo u Vashomu email]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення на тему: Надання довідки про те, що посвідчення батьків та дитини з багатодітної сім'ї не видавалося]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_header.html]
		    <h3>Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.</h3>			
			<p>
				Оброблено звернення № <a href="https://igov.org.ua/search?sID_Order=[sID_Order]">[sID_Order]</a>.<br>
				На жаль, Вам відмовлено у наданні довідки про те, що посвідчення батьків та дитини з багатодітної сім'ї не видавалося.<br />
				<b><i>${comment_1}</i></b>
			</p>
			<p>
		    	Звертаємо увагу, Ви можете заповнити заявку повторно на порталі <a href="https://igov.org.ua/service/64/general">iGov.org.ua</a>
			</p>
			<br>
			За наявності питань можете звернутись за наступними контактами<br>
			${sAddress}<br>
			${sWorkTime}<br>
			${sPhoneOrgan}<br><br>
			[pattern/mail/_common_signature_start.html]
			${sNameOrgan}.<br>
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_footer.html]]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA["${rishen}"]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow4" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow5" name="видача довідки" sourceRef="exclusivegateway1" targetRef="servicetask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${decision_1=='accept'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" name="відмова" sourceRef="exclusivegateway1" targetRef="servicetask5">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${decision_1=='reject'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow13" sourceRef="servicetask2" targetRef="endevent1"></sequenceFlow>
    <serviceTask id="servicetask1" name="Завантаження файлів" activiti:delegateExpression="#{fileTaskUpload}"></serviceTask>
    <sequenceFlow id="flow16" sourceRef="servicetask3" targetRef="servicetask4"></sequenceFlow>
    <sequenceFlow id="flow17" sourceRef="servicetask4" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow18" sourceRef="startevent1" targetRef="servicetask1"></sequenceFlow>
    <boundaryEvent id="boundaryerror1" name="Error" attachedToRef="servicetask1">
      <errorEventDefinition></errorEventDefinition>
    </boundaryEvent>
    <sequenceFlow id="flow19" sourceRef="boundaryerror1" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow20" sourceRef="servicetask5" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow21" sourceRef="servicetask1" targetRef="servicetask3"></sequenceFlow>
  </process>
